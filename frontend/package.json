{
  "name": "mindgraph-frontend",
  "private": true,
  "version": "5.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{vue,ts,js,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{vue,ts,js,json,css,scss}\"",
    "check": "vue-tsc --noEmit && npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "sync-version": "node scripts/sync-version.js",
    "prebuild": "npm run sync-version"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.48.1",
    "@vue-flow/minimap": "^1.5.4",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.7.9",
    "dompurify": "^3.2.4",
    "element-plus": "^2.13.0",
    "html-to-image": "^1.11.13",
    "lucide-vue-next": "^0.562.0",
    "markdown-it": "^14.1.0",
    "mitt": "^3.0.1",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.7",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
  }
}
